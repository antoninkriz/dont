{
  "manifest_version": 3,
  "name": "Don't Make Me Watch",
  "description":"Prevent window/tab change detection",
  "version":"3.0.0",
  "icons":{"16":"16.png","48":"48.png","128":"128.png"},
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js":["dont.js"],
      "run_at":"document_start",
      "all_frames": true
    }
  ]
}
